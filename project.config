[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/alarmbell (alarmbelll)
[access "refs/for/refs/*"]
	read = group user/alarmbell (alarmbelll)
	push = group user/alarmbell (alarmbelll)
	label-Code-Review = -2..+2 group user/alarmbell (alarmbelll)
	label-Verified = -1..+1 group user/alarmbell (alarmbelll)
	rebase = group user/alarmbell (alarmbelll)
	removeReviewer = group user/alarmbell (alarmbelll)
	submit = group user/alarmbell (alarmbelll)
[access "refs/heads/*"]
	read = group user/alarmbell (alarmbelll)
	create = group user/alarmbell (alarmbelll)
	push = +force group user/alarmbell (alarmbelll)
	pushMerge = group user/alarmbell (alarmbelll)
[access "refs/tags/*"]
	push = group user/alarmbell (alarmbelll)
	push = +force group user/alarmbell (alarmbelll)
